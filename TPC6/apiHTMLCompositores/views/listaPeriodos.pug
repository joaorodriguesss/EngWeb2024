extends layout

block content
	.w3-card-4
		header.w3-container.w3-blue 
			h1= titulo
		
		.w3-container
			br
			table.w3-table-all 
				tr
					th ID 
					th Nome

				each periodo in lista
					tr
						td
							a(href='/periodos/' + periodo.id)= periodo.id
						td= periodo.nome
			br
			
		footer.w3-container.w3-blue
			address Gerado por A100711 em #{data}
			a.w3-button.w3-blue(href='javascript:history.back()') Voltar
